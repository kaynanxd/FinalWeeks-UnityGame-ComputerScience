      voiceSound: {fileID: 8300000, guid: 978171ddf3ec45544b30fc895971e91f, type: 3}
    - characterName: ismayle
      text: "Agora que voc\xEA sabe do sistema de avalia\xE7\xE3o que \xE9 usado
        para jogos responda a afirmativa correta sobre ele:"
      portrait: {fileID: -5173085456471911758, guid: 7e8e84c8cf833c746bd999fb0087dc7c, type: 3}
      voiceSound: {fileID: 8300000, guid: 978171ddf3ec45544b30fc895971e91f, type: 3}
    choices:
    - text: "Ele s\xF3 avalia jogos digitais"
      targetNodeID: errou
      externalDialogue: {fileID: 0}
    - text: Ele avalia cardgames
      targetNodeID: errou
      externalDialogue: {fileID: 0}
    - text: "Ele avalia jogos tanto anal\xF3gicos como digitais "
      targetNodeID: acertou
      externalDialogue: {fileID: 0}
    - text: Hoje em dia ele e obsoleto
      targetNodeID: errou
      externalDialogue: {fileID: 0}
  - nodeID: acertou
