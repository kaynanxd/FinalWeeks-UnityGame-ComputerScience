  startNodeID: inicio
  nodes:
  - nodeID: inicio
    sentences:
    - characterName: ismayle
      text: Vamos ver se voce realmente aprendeu o conteudo !
      portrait: {fileID: -5173085456471911758, guid: 7e8e84c8cf833c746bd999fb0087dc7c, type: 3}
      voiceSound: {fileID: 8300000, guid: 978171ddf3ec45544b30fc895971e91f, type: 3}
    - characterName: ismayle
      text: Aqui vai a quarta pergunta !
      portrait: {fileID: -5173085456471911758, guid: 7e8e84c8cf833c746bd999fb0087dc7c, type: 3}
      voiceSound: {fileID: 8300000, guid: 978171ddf3ec45544b30fc895971e91f, type: 3}
    - characterName: ismayle
      text: "Sabendo dos tipos de jogos que existem, marque a op\xE7\xE3o do tipo
        de jogo que replica a vida real ou uma atividade dela pelo menos ?"
      portrait: {fileID: -5173085456471911758, guid: 7e8e84c8cf833c746bd999fb0087dc7c, type: 3}
      voiceSound: {fileID: 8300000, guid: 978171ddf3ec45544b30fc895971e91f, type: 3}
    choices:
    - text: "Gamifica\xE7\xE3o"
      targetNodeID: errou
      externalDialogue: {fileID: 0}
    - text: "Jogo s\xE9rio"
      targetNodeID: errou
      externalDialogue: {fileID: 0}
    - text: "Simula\xE7\xE3o"
      targetNodeID: acertou
      externalDialogue: {fileID: 0}
    - text: ' Jogo de entreterminento'
      targetNodeID: errou
      externalDialogue: {fileID: 0}
  - nodeID: acertou
    sentences:
    - characterName: ismayle
      text: Muito bem, agora entre no coliseu para me enfrentar de uma vez por todas!
      portrait: {fileID: -5173085456471911758, guid: 7e8e84c8cf833c746bd999fb0087dc7c, type: 3}
      voiceSound: {fileID: 8300000, guid: 978171ddf3ec45544b30fc895971e91f, type: 3}
    choices: []
  - nodeID: errou
    sentences:
    - characterName: ismayle
      text: Voce errou , Voce devia ter estudado mais ! irei repetir a pergunta ,
        esteja preparado
      portrait: {fileID: -5173085456471911758, guid: 7e8e84c8cf833c746bd999fb0087dc7c, type: 3}
      voiceSound: {fileID: 8300000, guid: 978171ddf3ec45544b30fc895971e91f, type: 3}
    choices: []
